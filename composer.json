{
    "name": "sandcore-dev/xml-analyzer",
    "description": "Analyze XML files to guess the range of values of attributes and text nodes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandcore Development",
            "email": "info@sandcore.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "SandcoreDev\\XmlAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SandcoreDev\\XmlAnalyzer\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
